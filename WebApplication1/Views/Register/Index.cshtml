@model PersonModel
<h2>Thông Tin Đăng Ký</h2>
<hr />
<form asp-action="XuLy" method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label>Họ tên</label>
        <input asp-for="FullName" class="form-group" />
    </div>
    <div class="form-group">
        <label>Ngày sinh</label>
        <input asp-for="BirthDay" type="date" class="form-group" />
    </div>
    <div class="form-group">
        <label>Hình đại diện</label>
        <input name="FHinh" type="file" class="form-control" />
    </div>
    <button type="submit" class="btn btn-danger">Đăng Ký</button>
</form>